char inByte = 0; // incoming serial byte
char outByte = 0; // incoming serial byte

void setup()
{
  // start serial port at 9600 bps:
  Serial.begin(9600);
  Serial2.begin(9600);
}

void loop()
{
  // if we get a valid byte, read analog ins:
  if (Serial.available() > 0) 
  {
    // get incoming byte:
    outByte = Serial.read();
    // send sensor values:
    Serial2.print(outByte);
  }
  if (Serial2.available() > 0) 
  {
    // get incoming byte:
    inByte = Serial2.read();
    // send sensor values:
    Serial.print(inByte);
  }
}
